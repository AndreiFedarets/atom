<UserControl x:Class="Atom.Client.Win.Views.ExtensionsExplorerView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Style="{DynamicResource PanelUserControl}"
             MinWidth="600" MinHeight="400">
    <Grid>
        <ListView x:Name="Assemblies">
           <ListView.View>
               <GridView>
                    <GridViewColumn Header="Name" DisplayMemberBinding="{Binding Name.Name}" Width="400" />
                    <GridViewColumn Header="Version" DisplayMemberBinding="{Binding Name.Version}" />
               </GridView>
           </ListView.View>
        </ListView>
    </Grid>
</UserControl>
